{
  "name": "ol-search",
  "description": "openlayers search extension",
  "version": "0.0.2",
  "author": "Yuhang Chen",
  "manifest_version": 3,
  "minimum_chrome_version": "99",
  "icons": {
    "16": "icon/ol.png",
    "48": "icon/ol.png",
    "128": "icon/ol.png"
  },
  "omnibox": {
    "keyword": "ol"
  },
  "permissions": [
    "activeTab"
  ],
  "background": {
    "service_worker": "service_worker.js"
  }
}