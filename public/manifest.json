{
  "manifest_version": 3,
  "name": "OL Search",
  "version": "0.1.1",
  "description": "openlayers search extension",
  "icons": {
    "16": "icons/ol-16.png",
    "32": "icons/ol-32.png",
    "48": "icons/ol-48.png",
    "128": "icons/ol-128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "OL Search",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage"
  ],
  "omnibox": {
    "keyword": "ol"
  }
}
